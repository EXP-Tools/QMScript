/**
 * 点击活动计量表
 */
Function click_cnt()
    is_ok = False
	FindColorEx 220,960,519,1050,"853408",1,0.8,intX,intY
	If intX > 0 And intY > 0 Then 
	    Delay 200
		call mouse_move(intX, intY)
		Delay 200
		LeftClick 1
		
		Delay 2000
		MoveTo 10, 1100
		Delay 200
		LeftClick 1
		
		is_ok = True
	End If
	click_cnt = is_ok
End Function
