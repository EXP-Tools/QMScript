/**
 * 延时操作（自动添加随机值）
 */
Function sleep(time)
	Delay time + rand_range(1000)
End Function

/**
 * 移动鼠标（自动添加随机值）
 */
Function mouse_move(x, y)
	MoveTo x + rand_range(40), y + rand_range(40)
End Function


/**
 * 获取范围内的随机值
 */
Function rand_range(range)
	Randomize
	rand_num = rnd() * range
	rand_num = int(rand_num)
	rand_range = rand_num
End Function
