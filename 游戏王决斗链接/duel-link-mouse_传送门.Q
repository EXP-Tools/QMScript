/**
 * 随机召唤怪兽
 */
Function exec_master()

	// 拉出卡片
    Call mouse_move(300, 1170)
    Delay 200
	LeftDown 1
	Delay 200
	Call mouse_move(300, 970)
	Delay 200
	LeftUp 1
	
	// 发动效果
	Call mouse_move(300, 970)
	Delay 200
	LeftClick 1
	
	Call sleep(1000)
	Call click_reconn()
			
End Function
