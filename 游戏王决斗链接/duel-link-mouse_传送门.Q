/**
 * 一个决斗流程
 */
Function duel()
    Call to_duel()
    Call to_end_duel()
	Call sleep(1000)
End Function
// ===============================================================================


Function to_duel()
	cnt = 0
    num = 0
	Do While True
	    cnt = cnt + 1
	    If cnt >= 8 Then
			If find_change_round() = True Then 
				Exit Do
			End If
		End If
		
		If click_csm_duel() = True Then 
			num = num + 1
		End If
			
		If num >= 4 Then 
			Exit Do
		End If
			
//		If cnt > 1 Then 
//			Call sleep(1000)
//			Call click_tips()
//		End If
		
		Call sleep(1000)
		Call click_center()
		
		
//		Call click_cnt()
	Loop
	
	cnt = 0
    Do While True
    	
    	Call click_center()
    	Call sleep(500)
    	
    	// 对方回合
    	If check_my_round() = False Then 
    		Call click_center()
    		Call click_csm_duel()
    		
			
		// 我方回合
    	Else 
    		Call sleep(1000)
    		Call take_card()
    		
    		Call sleep(1000)
    		Call exec_master()
    		
    		cnt = cnt + 1
    		If cnt > 1 Then
    			Call sleep(1000)
    			Call attack()
    		End If
    		
    		Call sleep(1000)
    		Call click_change_round()
    	End If

//    	Call click_effect()
//    	Call sleep(1000)
    	
    	If click_end_duel() = True Then 
			Call sleep(1000)
			Exit Do
		End If
    Loop
End Function

Function to_end_duel()
    cnt = 0
    num = 0
	Do While True
	    cnt = cnt + 1
	    
	    If num >= 2 Then 
			Exit Do
		End If
		
		If click_next_step() = True Then 
			num = num + 1
		End If
		
//		If click_cnt() Then 
//			num = num + 1
//			Exit Do
//		End If
		
		If cnt >= 30 Then
			Exit Do
		End If
		
		Call sleep(1000)
		If click_tips() = False Then 
			Call click_reconn()
		End If
		Call click_center()
	Loop
	
	Call sleep(1000)
	
End Function
