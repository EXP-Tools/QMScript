[Script]
// ===============================================================================

Event Form.Shop1.Click
    Call main(1)
End Event

Event Form.Shop2.Click
    Call main(2)
End Event

Event Form.Shop3.Click
    Call main(3)
End Event

Event Form.Shop4.Click
    Call main(4)
End Event

Event Form.Shop5.Click
    Call main(5)
End Event

Event Form.Shop6.Click
    Call main(6)
End Event

// ===============================================================================



Sub main(SHOP_ID)

	MoveTo 100, 100
	LeftClick 1
		
	x = 400 + (150 * (SHOP_ID - 1))
	y = 330
	
	isRun = True
	Do
		//  点击商店头像
		MoveTo x, y
		LeftClick 1
		Delay 200
		
		// 点击购买
		MoveTo 680, 415
		LeftClick 1
		Delay 500
		
		// 点击打开
		LeftClick 1
		Delay 7000	// 等待卡包打开完成
		
		// 返回商店页
		RightClick 1
		Delay 500
		
		FindPic 0,0,1366,768,"Attachment:\shop.bmp",0.9,intX,intY
		If intX <= 0 Or intY <= 0 Then 
			isRun = False
		End If

	Loop While isRun
	
End Sub



